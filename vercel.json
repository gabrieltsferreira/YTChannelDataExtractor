{
    "devCommand": "flask run -h localhost -p 3000",
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "installCommand": "pip install -r requirements.txt"
  }